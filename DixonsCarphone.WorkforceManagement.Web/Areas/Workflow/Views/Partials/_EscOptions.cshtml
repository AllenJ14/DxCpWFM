@model List<DixonsCarphone.WorkforceManagement.Business.Entities.sp_EscalationOptions_Result>
@{
    bool first = true;
}

@if (Model.Count() > 0)
{
    foreach (var item in Model)
    {
        <li>
           <a href="#" data-toggle="modal" data-target="#sendConfirm" data-action="@item.Level">@item.Description</a>
        </li>
        if (first)
        {
            <li role="separator" class="divider"></li>
            first = !first;
        }
    }
}
else
{
    <li>Error...</li>
}
